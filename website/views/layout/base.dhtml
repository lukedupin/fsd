<HTML>
<meta charset="iso-8859-1">
<HEAD>
<link rel="stylesheet" type="text/css" href="/static/share.css" />
<link rel="stylesheet" type="text/css" href="/static/font/stylesheet.css" />
{%block css%}
{%endblock%}
<title>{% block title %}Fluid software designs{% endblock %}</title>
</HEAD>
<BODY class="body">
  <div class="{%block main_page%}home_main_page{%endblock%}">

    {% block navigration %}
    <div class="header">
      <div class="header_left">
      </div>
      <div class="header_angle">
        <div class="header_logo"></div>
      </div>

      <div class="header_right">
        <div class="header_nav_container">
          <div class="{%block home_link%}header_nav_container_item{%endblock%}">
            <a href="/">
              <div class="header_nav_number">01</div>
              <div class="header_nav_text">Home</div>
            </a>
          </div>
        </div>
        <div class="header_nav_container">
          <div class="{%block services_link%}header_nav_container_item{%endblock%}">
            <a href="/services">
              <div class="header_nav_number">02</div>
              <div class="header_nav_text">Services</div>
            </a>
          </div>
        </div>
        <div class="header_nav_container">
          <div class="{%block work_link%}header_nav_container_item{%endblock%}">
            <a href="/work">
              <div class="header_nav_number">03</div>
              <div class="header_nav_text">Work</div>
            </a>
          </div>
        </div>
        <div class="header_nav_container">
          <div class="header_nav_container_item">
            <a href="/ethos">
              <div class="header_nav_number">04</div>
              <div class="header_nav_text">Ethos</div>
            </a>
          </div>
        </div>
        <div class="header_nav_container">
          <div class="header_nav_container_item">
            <a href="/contact">
              <div class="header_nav_number">05</div>
              <div class="header_nav_text">Contact</div>
            </a>
          </div>
        </div>

      </div>
    </div>
    <div style="clear: both;"></div>
    {% endblock %}

    {% block header %}
    {% endblock %}

    {% block body %}
    {% endblock %}

    {% block footer %}
    {% endblock %}

    <div class="footer_left">
      <div class="footer_logo"></div>
      <div class="footer_copyright">Copyright 2012</div>
    </div>
    <div class="footer_right">
      <div class="footer_text">
        <div class="footer_text_item"><a href="/">Home</a></div>
        <div class="footer_text_item">//</div>
        <div class="footer_text_item"><a href="/services">Services</a></div>
        <div class="footer_text_item">//</div>
        <div class="footer_text_item"><a href="/work">Work</a></div>
        <div class="footer_text_item">//</div>
        <div class="footer_text_item"><a href="/ethos">Ethos</a></div>
        <div class="footer_text_item">//</div>
        <div class="footer_text_item"><a href="/contact">Contact</a></div>
      </div>
    </div>
  </div>
</BODY>
</HTML>
